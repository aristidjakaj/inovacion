  <body>
    <% include partials/navbar.ejs %>
    <div class="container">
      <div id="home-page-first-section">
        <h1 class="text-light" style="font-size: 500%; margin-bottom: 3%;">iBusiness</h1>
        <h4 class="text-light" style="font-size: 200%; margin-bottom: 2%; font-family: cursive;">Better connections, better solutions</h4>
        <a href="/account" role="button" class="btn btn-danger" style="width: 18%; font-size: 150%;">Get Started</a>
      </div>
      <div id="home-page-second-section">
        <div>
